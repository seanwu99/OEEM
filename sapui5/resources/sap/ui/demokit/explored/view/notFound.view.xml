<mvc:View
	controllerName="sap.ui.demokit.explored.view.notFound"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns="sap.m">
	<Page
		title="Not Found"
		showNavButton="{device>/isPhone}"
		navButtonPress="onNavBack"
		enableScrolling="false" >
		<content>
			<Text text="The requested entity '{/entityName}' is unknown to the explored app." textAlign="Center" width="100%"></Text>
		</content>
	</Page>
</mvc:View>